{
  "name": "lena",
  "version": "0.1.0",
  "description": "A social app for meeting people in real life through shared activities",
  "private": true,
  "workspaces": [
    "mobile",
    "web",
    "backend",
    "shared"
  ],
  "scripts": {
    "postinstall": "husky install || echo 'Husky install failed, skipping...'",
    "install:all": "npm install --workspaces",
    "dev:mobile": "npm run dev --workspace=mobile",
    "dev:web": "npm run dev --workspace=web",
    "dev:backend": "npm run dev --workspace=backend",
    "dev:all": "concurrently --kill-others-on-fail --prefix-colors cyan,magenta,yellow \"npm:dev:backend\" \"npm:dev:web\"",
    "dev": "npm run dev:all",
    "build:web": "npm run build --workspace=web",
    "build:mobile": "npm run build --workspace=mobile",
    "build:backend": "npm run build --workspace=backend",
    "build:all": "npm run build --workspaces --if-present",
    "build": "npm run build:all",
    "test:all": "npm run test --workspaces --if-present",
    "test": "npm run test:all",
    "test:watch": "npm run test:watch --workspaces --if-present",
    "lint:all": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint": "npm run lint:all",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck:all": "npm run typecheck --workspaces --if-present",
    "typecheck": "npm run typecheck:all",
    "validate": "npm run lint && npm run typecheck && npm run test",
    "clean:deps": "npm run clean --workspaces --if-present && rimraf node_modules",
    "clean:build": "rimraf */dist */build */.next",
    "clean:all": "npm run clean:deps && npm run clean:build",
    "clean": "npm run clean:all",
    "reset": "npm run clean:all && npm install",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:build": "docker-compose build",
    "db:migrate": "npm run db:migrate --workspace=backend",
    "db:seed": "npm run db:seed --workspace=backend",
    "db:studio": "npm run db:studio --workspace=backend",
    "db:reset": "npm run db:reset --workspace=backend"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/weave0/Lena.git"
  },
  "keywords": [
    "social",
    "meetup",
    "activities",
    "travel",
    "react-native",
    "real-life-connections"
  ],
  "author": "Lena",
  "license": "PROPRIETARY",
  "dependencies": {
    "@types/multer": "^1.4.11",
    "@vitejs/plugin-react": "^5.1.1",
    "vite": "^7.2.4"
  }
}
